{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./"],
    "types": ["node"]
  },
  "include": ["src/**/*.ts","./express.d.ts"],
  "exclude": ["node_modules"]
}
